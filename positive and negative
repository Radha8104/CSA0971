import java.util.*;
class positive 
{
	public static void main(String arg[]){
	int n;
	Scanner sc=new Scanner(System.in);
	System.out.println("enter the value of n:");
	n=sc.nextInt();
	if(n>0){
	System.out.println("positive number");
	}else if(n<0){
	System.out.println("negative number");
	}
	else{
	System.out.println("zero");
	}
	}
}
